<div class="user_subheader">
  <span class="<%= status.downcase %>_all">
    <%= status %>
  </span>
  <span class="add_media_button">
    <% if user == current_user %>
      <%= button_to 'Add Media', new_medium_path, :method => :get, class: "new_media #{status.downcase} button tiny radius" %>
    <% end %>
  </span>
</div>